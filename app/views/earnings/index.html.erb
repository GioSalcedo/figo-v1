<div class="container">
          <div class="row">
            <div class="col-lg-12 col-md-6">
              <div class="card rounded-0">
                <div class="card-header bg-light">
                  <h6 class="font-weight-bold mb-0">Entrada de Dinero</h6>
                </div>
  <div class="card-body">
    <table class="tabla">
      <thead>
          <tr>
            <th>Fecha</th>
            <th>Descripción</th>
            <th>Categoría</th>
            <th>Monto</th>
          </tr>
      </thead>
    <% current_user.businesses.each do |business| %>
      <% business.accounts.each do |account| %>
        <% account.earnings.each do |earning| %>
                    <tbody>
                      <tr>
                        <td><%= earning.date %></td>
                        <td><%= earning.note %></td>
                        <td><%= earning.category %></td>
                        <td>$<%= earning.balance %></td>
                      </tr>
                    </tbody>
                </div>
              </div>
            </div>
          </div>
        <% end %>
      <% end %>
    <% end %>
  </div>
